#리스트
subway= [10,20,30]
print(subway)
subway=["유","조","박"]
print(subway)
print(subway.index("조"))
subway.append("하")#추가하기
print(subway)
subway.insert(1,"정")#끼워넣기
print(subway)
print(subway.pop())#뒤에하나 빼기
print(subway)
subway.append("유")
print(subway)
print(subway.count("유"))

#정렬
list=[5,2,3,4,1]
list.sort()#오름차순
print(list)
list.reverse()#내림차순
print(list)
#list.clear()#다 지우기
print(list)

mixlist=["조",20,True]
print(mixlist)

#리스트 확장
list.extend(mixlist)
print(list)
